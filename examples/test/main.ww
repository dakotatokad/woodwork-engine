kb = knowledge_base chroma {
    api_key: $OPENAI_API_KEY
    file_to_embed: "./endpoint.txt"
}

model = llm openai {
    model: "gpt-4o-mini"
    api_key: $OPENAI_API_KEY
    knowledge_base: kb
}

planner = decomposer llm {
    api_key: $OPENAI_API_KEY
    tools: [model, kb]
    workflow: "./workflow.json"
}

in = input command_line {
    to: planner
}