name1 = knowledge_base chroma {
    client: "local"
}

model2 = llm openai {
    model: "gpt-4o"
    knowledge_base: name1
    api_key: $OPENAI_API_KEY
}

endpoint = api web {
    url: "http://localhost:3000"
    documentation: "endpoint.txt"
}

decomposer1 = decomposer llm {
    api_key: $OPENAI_API_KEY
    api: endpoint
}

in = input command_line {
    to: decomposer1
}